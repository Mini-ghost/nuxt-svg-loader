{
  "name": "nuxt-svgo-loader",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@8.12.0",
  "description": "Nuxt module to load SVG files as Vue components, using SVGO for optimization.",
  "author": {
    "name": "Alex Liu <dsa1314@gmail.com>",
    "url": "https://github.com/Mini-ghost"
  },
  "license": "MIT",
  "homepage": "https://github.com/Mini-ghost/nuxt-svgo-loader#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mini-ghost/nuxt-svgo-loader.git"
  },
  "bugs": {
    "url": "https://github.com/Mini-ghost/nuxt-svgo-loader/issues"
  },
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "svgo",
    "svg"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "stub": "nuxt-build-module build --stub",
    "build": "pnpm dev:prepare && pnpm build:module && pnpm build:client",
    "build:client": "nuxi generate client",
    "build:module": "nuxt-build-module build",
    "dev": "nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "prepack": "pnpm build",
    "prepare": "husky install",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^1.0.6",
    "@nuxt/kit": "^3.8.2",
    "birpc": "^0.2.14",
    "fast-glob": "^3.3.2",
    "pathe": "^1.1.1",
    "perfect-debounce": "^1.0.0",
    "scule": "^1.1.1",
    "sirv": "^2.0.3",
    "svgo": "^3.1.0",
    "vite-svg-loader": "^5.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.5",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.2",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "bumpp": "^9.2.1",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nuxt": "^3.8.2",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "nuxt-svgo-loader": "link:."
  }
}
